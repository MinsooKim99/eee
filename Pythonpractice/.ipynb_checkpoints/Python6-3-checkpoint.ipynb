   "source": [
    "def pageing(x, y):\n",
    "    if x % y == 0:\n",
    "        return x // y\n",
    "    else:\n",
    "        return x // y + 1\n",
    "\n",
    "print(pageing(5, 10))\n",
    "print(pageing(15, 10))\n",
    "print(pageing(20, 10))"
   ]
  }
