   "metadata": {},
   "outputs": [],
   "source": [
    "import sys\n",
    "tabContents=sys.argv[1]\n",
    "notTabContents=sys.argv[2]\n",
    "f=open(tabContents,'r')\n",
    "tabText=f.read()\n",
    "f.close()\n",
    "notTabText=tabText.replace(\"\\t\",\" \"*4)\n",
    "f=open(notTabContents,'w')\n",
    "f.write(notTabText)\n",
    "f.close()"
   ]
  }
